{
  "patterns": {
    "percentage": "\\b\\d+(?:\\.\\d+)?%",
    "money": "\\$\\d+(?:[.,]\\d+)?[KMB]?\\b",
    "multiplier": "\\b\\d+x\\b",
    "plus": "\\b\\d+\\+",
    "range": "from \\d+ to \\d+",
    "comparison": "(?:increased|reduced|improved|decreased|enhanced|boosted|cut|accelerated).*?by\\s+\\d+",
    "time": "\\d+\\s+(?:days|weeks|months|years)",
    "count": "\\d+\\s+(?:users|customers|clients|projects|teams|people|employees|members|engineers)"
  },
  "quality_weights": {
    "money": 1.0,
    "percentage": 1.0,
    "comparison": 0.9,
    "range": 0.8,
    "count": 0.7,
    "time": 0.7,
    "multiplier": 0.8,
    "plus": 0.6
  }
}
